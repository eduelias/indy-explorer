{
	"prettier.semi": true,
	"cSpell.words": ["verkey"],
	"editor.defaultFormatter": "dbaeumer.vscode-eslint",
	"editor.formatOnSave": true,
	"eslint.enable": true,
	"eslint.alwaysShowStatus": true,
	"editor.codeActionsOnSave": {
	  "source.fixAll.eslint": true
	},
	"editor.semanticHighlighting.enabled": false
}
